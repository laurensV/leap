{
    "name": "leap/leap",
    "description": "leap is an open source PHP framework.",
    "type": "project",
    "license": "GPL-2.0+",
    "autoload": {
        "psr-4": {
            "Leap\\":""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://localhost/satis/web/"
        }
    ],
    "require": {
        "davidbarratt/custom-installer": "1.0.*@dev",
        "leap/admin": "dev-master",
        "leap/alias": "dev-master",
        "leap/less": "dev-master",
        "leap/pluginmanager": "dev-master"
    },
    "extra": {
        "custom-installer": {
            "core/": ["type:leap-core"],
            "plugins/{$name}/": ["type:leap-plugin"],
            "testfolder/": ["composer/composer"]
        }
    },
    "config": {
        "vendor-dir": "libraries"
    }
}
