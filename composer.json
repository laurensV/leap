{
    "name": "frameworkname/frameworkname",
    "description": "frameworkname is an open source PHP framework.",
    "type": "project",
    "license": "GPL-2.0+",
    "autoload": {
        "psr-4": {
            "Frameworkname\\":""
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "frameworkname/admin",
                "description": "admin plugin for frameworkname2",
                "version": "3.0",
                "type": "frameworkname-plugin",
                "dist": {
                    "url": "https://github.com/laurensV/php-framework/raw/master/plugins/admin.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "davidbarratt/custom-installer": "1.0.*@dev",
        "frameworkname/admin": "3.0",
        "oyejorge/less.php": "~1.7.0.9"
    },
    "extra": {
        "custom-installer": {
            "core/": ["type:frameworkname-core"],
            "plugins/{$name}/": ["type:frameworkname-plugin"]
        }
    },
    "config": {
        "vendor-dir": "libraries"
    }
}
